{
    "name": "zerai/avsym",
    "type": "project",
    "description": "Av server + sym poc.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "artgris/maintenance-bundle": "^1.2",
        "babdev/pagerfanta-bundle": "^4.0",
        "bamarni/composer-bin-plugin": "^1.8",
        "ecotone/dbal": "^1.85",
        "ecotone/jms-converter": "^1.85",
        "ecotone/symfony-bundle": "^1.85",
        "pagerfanta/twig": "^4.1",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Publishing\\": "_publishing/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Publishing\\Tests\\": "_publishing/tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "cs": "./vendor/bin/ecs",
        "cs:fix": "./vendor/bin/ecs --fix",
        "tu": "./bin/phpunit --testsuite=unit"
    }
}
